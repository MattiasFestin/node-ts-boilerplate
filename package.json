{
  "name": "node-boilerplate",
  "version": "1.0.0",
  "description": "A TS node boilerplate project",
  "main": "build/src/Main.js",
  "scripts": {
    "test": "nyc --exclude 'build/tests/*.js' --check-coverage --lines 100 ava build/tests/*.js",
    "watch:test": "nodemon -x 'npm test'",
    "doc": "typedoc --module commonjs --out ./build/doc/ ./src/Main.ts"
  },
  "author": "Mattias Festin",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.14.0",
    "nodemon": "^1.9.2",
    "nyc": "^6.4.4",
    "typedoc": "^0.3.12",
    "nodemon": "^1.10.0"
  }
}
